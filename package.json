{
  "name": "personal-site",
  "description": "Sergio Xalambrí personal website",
  "scripts": {
    "dev": "node server.js",
    "build": "next build",
    "preexport": "npm run build",
    "export": "next export -o portfolio",
    "postexport": "cp .next/service-worker.js portfolio",
    "alias": "now alias",
    "clean": "now rm portfolio --safe --yes",
    "prestart": "npm run export",
    "start": "serve portfolio",
    "predeploy": "npm run export",
    "deploy": "now deploy portfolio",
    "postdeploy": "npm run alias && npm run clean"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sergiodxa/personal-site.git"
  },
  "keywords": [],
  "author": "Sergio Daniel Xalambrí <sergiodxa@gmail.com> (https://sergio.now.sh/)",
  "license": "MIT",
  "private": true,
  "bugs": {
    "url": "https://github.com/sergiodxa/personal-site/issues"
  },
  "homepage": "https://sergio.now.sh",
  "dependencies": {
    "date-fns": "^1.28.5",
    "lodash.memoize": "^4.1.2",
    "module-alias": "^2.0.1",
    "next": "beta",
    "preact": "^8.2.1",
    "preact-compat": "^3.16.0"
  },
  "devDependencies": {
    "react": "15.6.1",
    "react-dom": "15.6.1",
    "serve": "^6.0.3",
    "sw-precache-webpack-plugin": "^0.11.4"
  }
}
