{
  "name": "personal-site",
  "description": "Sergio Xalambrí personal website",
  "main": "server/index.js",
  "bin": "./site",
  "scripts": {
    "dev": "nodemon .",
    "build": "next build",
    "build:rss": "node scripts/build-rss.js",
    "dist": "pkg index.js --output ./site --target linux",
    "start": "NODE_ENV=production node .",
    "deploy": "now deploy portfolio --token $TOKEN -A ../now.json",
    "alias": "now alias --token $TOKEN",
    "now-build": "echo 'Built on CI'",
    "now-start": "./site"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sergiodxa/personal-site.git"
  },
  "keywords": ["nextjs", "blog", "personal-website", "personal-site"],
  "author": {
    "name": "Sergio Daniel Xalambrí",
    "email": "hello@sergiodxa.com",
    "url": "https://sergiodxa.com/"
  },
  "license": "MIT",
  "private": true,
  "bugs": {
    "url": "https://github.com/sergiodxa/personal-site/issues"
  },
  "homepage": "https://sergiodxa.com",
  "devDependencies": {
    "@platzi/markdown": "^1.3.3",
    "apollo-cache-inmemory": "^1.1.9",
    "apollo-cache-persist": "^0.1.1",
    "apollo-client": "^2.2.5",
    "apollo-link-http": "^1.3.3",
    "babel-plugin-transform-define": "^1.3.0",
    "babili-webpack-plugin": "^0.1.2",
    "classnames": "^2.2.5",
    "data-prefetch-link": "^1.1.2",
    "date-fns": "^1.28.5",
    "express": "^4.16.2",
    "graphql": "^0.13.1",
    "graphql-tag": "^2.7.3",
    "himalaya": "^1.0.0",
    "html-entities": "^1.2.1",
    "html-word-count": "^2.0.0",
    "isomorphic-fetch": "^2.2.1",
    "js-yaml": "^3.10.0",
    "lodash.memoize": "^4.1.2",
    "lru-cache": "^4.1.1",
    "markdown-it-codesandbox": "^1.0.0",
    "markdown-it-headinganchor": "^1.3.0",
    "markdown-it-highlightjs": "^3.0.0",
    "markdown-it-mentions": "^1.0.0",
    "markdown-it-regexp": "^0.4.0",
    "next": "5.0.0",
    "next-offline": "^2.1.1",
    "nodemon": "^1.14.1",
    "now-env": "^3.0.2",
    "nprogress": "^0.2.0",
    "pkg": "^4.3.0",
    "prop-types": "^15.6.0",
    "prop-types-exact": "^1.1.1",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-ga": "^2.2.0",
    "recompose": "^0.26.0",
    "slugify": "^1.2.1",
    "strip": "^3.0.0"
  },
  "pkg": {
    "assets": [".next", "pages"],
    "scripts": [".next/dist/**/*.js"]
  }
}
