{
  "name": "personal-site",
  "description": "Sergio Xalambrí's personal website",
  "scripts": {
    "dev": "next dev -p 3001",
    "build": "next build",
    "preexport": "npm run build",
    "export": "next export -o dist",
    "postexport": "cp .next/service-worker.js dist && cp static/robots.txt dist && npm run build:rss && npm run build:sitemap",
    "build:rss": "node scripts/build-rss.js",
    "build:sitemap": "node scripts/build-sitemap.js",
    "alias": "now alias",
    "prestart": "npm run build",
    "prestart:static": "npm run export",
    "start": "next start -p 3001",
    "start:static": "serve dist",
    "deploy": "now deploy dist -A ../now.json",
    "postdeploy": "npm run alias"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sergiodxa/personal-site.git"
  },
  "keywords": [
    "nextjs",
    "blog",
    "personal-website",
    "personal-site"
  ],
  "author": {
    "name": "Sergio Daniel Xalambrí",
    "email": "hello@sergiodxa.com",
    "url": "https://sergiodxa.com/"
  },
  "license": "MIT",
  "private": true,
  "bugs": {
    "url": "https://github.com/sergiodxa/personal-site/issues"
  },
  "homepage": "https://sergiodxa.com",
  "dependencies": {
    "@mdx-js/mdx": "^0.8.1",
    "@sergiodxa/analytics": "^1.0.0",
    "@sergiodxa/case": "^1.0.0",
    "@sergiodxa/clipboard": "^1.0.0",
    "@sergiodxa/fs": "^1.1.0",
    "@sergiodxa/icons": "^1.0.0",
    "@sergiodxa/map": "^1.0.0",
    "@sergiodxa/palette": "^1.0.0",
    "@sergiodxa/request": "^1.0.0",
    "@sergiodxa/terminal": "^1.0.0",
    "@sergiodxa/types": "^1.0.0",
    "@sergiodxa/ui": "^1.0.1",
    "@zeit/next-mdx": "^1.0.0",
    "apollo-cache-inmemory": "^1.1.9",
    "apollo-cache-persist": "^0.1.1",
    "apollo-client": "^2.2.5",
    "apollo-link-http": "^1.3.3",
    "babili-webpack-plugin": "^0.1.2",
    "classnames": "^2.2.5",
    "date-fns": "^1.28.5",
    "globby": "^8.0.1",
    "graphql": "^0.13.1",
    "graphql-tag": "^2.7.3",
    "isomorphic-fetch": "^2.2.1",
    "lodash.memoize": "^4.1.2",
    "next": "^6.0.1-canary.1",
    "next-offline": "^2.1.1",
    "now-env": "^3.0.4",
    "nprogress": "^0.2.0",
    "prop-types": "^15.6.0",
    "prop-types-exact": "^1.1.1",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "recompose": "0.27.0",
    "remark-abbr": "^1.1.2",
    "remark-emoji": "^2.0.1",
    "slugify": "^1.2.1"
  },
  "devDependencies": {
    "express": "^4.16.2",
    "nodemon": "^1.14.1",
    "serve": "^6.0.0"
  }
}
