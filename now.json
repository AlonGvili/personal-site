{
  "name": "personal-site",
  "alias": "site.sergiodxa.com",
  "type": "static",
  "env": {
    "NODE_ENV": "@env"
  },
  "rules": [
    { "pathname": "/es6/**", "dest": "https://sergiodxa.github.io" },
    { "pathname": "/css4/**", "dest": "https://sergiodxa.github.io" },
    {
      "pathname": "/graphql",
      "method": ["POST"],
      "dest": "sergio-api.now.sh"
    },
    { "dest": "portfolio.now.sh" }
  ]
}
